<odoo>
  <record model="ir.ui.view" id="ean_form">
    <field name="name">EAN form</field>
    <field name="model">script.ean</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="current_code"/>
          </group>
          <group>
            <field name="computed_key"/>
          <field name="final_code"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
