<odoo>
    <!-- Added field in subscription -->
    <record id="amb_sale_subscription_inherit_sale_order_view_form" model="ir.ui.view">
        <field name="priority" eval="10"/>
        <field name="name">amb.sale.subscription.inherit.sale.order.view.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale_subscription.sale_subscription_primary_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='pricelist_id']" position="after">
                <field name="oci_point_of_sale"/>
                <field name="payment_instrument_id"/>
            </xpath>
        </field>
    </record>
</odoo>